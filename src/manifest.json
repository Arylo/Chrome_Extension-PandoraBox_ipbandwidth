{
    "name": "PandoraBox ipbandwidth",
    "manifest_version": 2,
    "version": "1.1",
    "description": "I can see the IP bandwidth",
    "icons": {
        "32": "images/logo_32.png",
        "64": "images/logo_64.png",
        "128": "images/logo_128.png"
    },
    "browser_action": {
        "default_icon": "images/logo_32.png",
        "default_popup": "views/popup.html"
    },
    "permissions": [
        "http://*/*"
    ]
}
